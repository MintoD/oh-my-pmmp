{
    "name": "thebigcrafter/oh-my-pmmp",
    "description": "A plugin manager for PocketMine-MP downloads plugin from PocketMine-MP official plugin repository",
    "type": "project",
    "require": {
        "php": "^8.0",
        "cortexpe/commando": "dev-master",
        "react/promise": "2.9.0",
      "ext-curl": "*"
    },
    "require-dev": {
        "pocketmine/pocketmine-mp": "^4.0",
        "phpstan/phpstan": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "license": "GPL-3.0-only",
    "config": {
        "platform": {
            "php": "8.0.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "thebigcrafter\\OhMyPMMP\\": "src/thebigcrafter/OhMyPMMP"
        }
    },
    "authors": [
        {
            "name": "MintoD",
            "email": "tobythedev7@outlook.com"
        }
    ],
    "scripts": {
        "fix-cs": "./vendor/bin/php-cs-fixer fix",
        "analyse": "./vendor/bin/phpstan analyse -c phpstan.neon.dist"
    }
}
